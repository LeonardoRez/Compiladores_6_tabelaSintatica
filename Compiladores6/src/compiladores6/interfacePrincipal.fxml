<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="414.0" prefWidth="310.0" styleClass="mainFxmlClass" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.65" fx:controller="compiladores6.InterfacePrincipalController">
    <stylesheets>
        <URL value="@interfaceprincipal.css" />
    </stylesheets>
   <children>
      <Button layoutX="5.0" layoutY="24.0" mnemonicParsing="false" onAction="#pickTabela" prefHeight="26.0" prefWidth="300.0" text="Escolher arquivo" />
      <TextField fx:id="tfAddToken" layoutX="5.0" layoutY="95.0" onAction="#addToken" prefHeight="26.0" prefWidth="300.0" promptText="ex: id" />
      <Button layoutX="5.0" layoutY="129.0" mnemonicParsing="false" onAction="#addToken" prefHeight="26.0" prefWidth="300.0" text="Add token" />
      <TextField fx:id="entrada" editable="false" layoutX="5.0" layoutY="186.0" prefHeight="26.0" prefWidth="300.0" />
      <Button layoutX="5.0" layoutY="217.0" mnemonicParsing="false" onAction="#resetaTokens" prefHeight="26.0" prefWidth="300.0" text="Reset tokens" />
      <TextField fx:id="varInicial" layoutX="5.0" layoutY="306.0" prefHeight="26.0" prefWidth="300.0" promptText="Variavel inicial" />
      <Button layoutX="5.0" layoutY="338.0" mnemonicParsing="false" onAction="#submit" prefHeight="26.0" prefWidth="300.0" text="Submeter tokens" />
      <Label fx:id="log" layoutX="5.0" layoutY="375.0" prefHeight="16.0" prefWidth="300.0" textFill="#d71414" />
   </children>
</AnchorPane>
